#!/usr/bin/env node

var nesly = require('../');

var argv = require('optimist')
    .usage('Compile down to 6502 asm.\nUsage: $0')
    .demand('f')
    .alias('f', 'file')
    .describe('f', 'File to write to')
    .argv;

var fs = require('fs');
var s = fs.createWriteStream(argv.file);

s.write(nesly());
